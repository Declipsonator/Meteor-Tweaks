{
  "schemaVersion": 1,
  "id": "addon-template",
  "version": "${version}",
  "name": "Meteor Tweaks",
  "description": "A Meteor Client Addon that adds tweaks to modules and settings.",
  "authors": [
    "Declipsonator"
  ],
  "contact": {
    "repo": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "icon": "assets/meteortweaks/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "me.declipsonator.meteortweaks.MeteorTweaks"
    ]
  },
  "mixins": [
    "tweaks.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "155,1,1"
  },
  "depends": {
    "java": ">=16",
    "minecraft": ">=@mc_version@",
    "meteor-client": "*"
  }
}
